#Managed by puppet
Server=<%= @servers.join(', ') %>
<%- if @activeservers.any? then %>
ServerActive=<%= @activeservers.join(', ') %>
<%-end -%>
ListenPort=<%= @port %>
Hostname=<%= @fqdn %>
StartAgents=<%= @start_agents %>
DebugLevel=<%= @agent_debug_level %>
PidFile=<%= @pid_file %>
LogFile=<%= @log_file %>
Timeout=<%= @agent_timeout %>
Include=<%= @include_dir %>
